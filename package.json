{
  "name": "Prueba-Node",
  "version": "1.0.0",
  "scripts": {
    "build": "npm-run-all clean copy lint tsc",
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "copy": "cpx package.json dist/ && cpx package-lock.json dist/ ",
    "start": "cross-env ENV_FILE=production node -r module-alias/register start.js",
    "start-dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\"",
    "deploy": "bluemix app push"
  },
  "_moduleAliases": {
    "@shared": "shared",
    "@server": "Server"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cpx": "^1.5.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "tslib": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.4.4",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.3",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
